{
  "compilerOptions": {
    "module": "nodenext",
    "useUnknownInCatchVariables": false,
    "allowImportingTsExtensions": true,
    "noEmit": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "strict": true,
    "baseUrl": "src",
    "paths": {
      "@/*": ["*"],
      "result": ["utils/result/index.ts"]
    }
  },
  "include": [
    "src",
    "types.d.ts"
, "trash/messageHandler.ts"  ],
  "exclude": ["trash"]
}